{
    "name": "na-map-one",
    "packageManager": "yarn@3.6.1",
    "version": "0.0.0",
    "description": "Naval Action map",
    "scripts": {
        "clean:src:js": "find ./src/js -name \"*.ts\" -type f -regextype posix-egrep -regex \".*/(browser|common|node)/.+\" -exec bash -c 'for file in \"${1%.ts}\".js \"${1%.ts}\".js.map; do [ -e \"$file\" ] && rm -f \"$file\"; done' - '{}' ';'",
        "docs": "run-p 'docs:*'",
        "docs:js": "documentation build src/js/**.js build/**.mjs -f html -o ./doc-js",
        "docs:sass": "sassdoc src/scss -d ./doc-sass",
        "helper:loc": "npx cloc --by-file --not-match-f='.d.ts' --quiet ./src/js",
        "helper:size:css": "find public/ -iname '*.css' -exec du -bc {} + | sort -k 2 | sed 's|public/||'",
        "helper:size:js": "find public/ -iname '*.js' -not -iname 'data-*.js' -exec du -bc {} + | sort -k 2 | sed 's|public/||'",
        "helper:size:largest": "du -a ~/ -BM | sort -n -r | head -n 15",
        "lint": "run-s 'lint:*'",
        "lint:css": "stylelint --config .stylelintrc \"src/scss/*.scss\"",
        "lint:ejs": "ejslint src/ejs",
        "lint:js": "eslint -c .eslintrc.cjs --ext .ts,.tsx src/js",
        "main:release": "run-s build:load build:data:convert release deploy:netlify",
        "prod": "TS_NODE_PROJECT='webpack/tsconfig-for-webpack-config.json' webpack --config webpack/webpack.config.ts --mode=production --color",
        "release": "standard-version",
        "start": "TS_NODE_PROJECT='webpack/tsconfig-for-webpack-config.json' webpack serve --config webpack/webpack.config.ts --mode=development --color --hot --open",
        "typecheck": "tsc --noEmit",
        "update": "yarn set version stable && yarn upgrade-interactive"
    },
    "sass": "src/scss/main.scss",
    "dependencies": {
        "@d3fc/d3fc-label-layout": "^7.0.3",
        "@popperjs/core": "^2.11.8",
        "bootstrap": "^5.2.3",
        "bootstrap-select": "https://github.com/felixvictor/bootstrap-select#dev",
        "d3-area-label": "^1.5.0",
        "d3-array": "^3.2.4",
        "d3-axis": "^3.0.0",
        "d3-color": "^3.1.0",
        "d3-delaunay": "^6.0.4",
        "d3-drag": "^3.0.0",
        "d3-format": "^3.1.0",
        "d3-hierarchy": "^3.1.2",
        "d3-interpolate": "^3.0.1",
        "d3-scale": "^4.0.2",
        "d3-selection": "^3.0.0",
        "d3-shape": "^3.2.0",
        "d3-tile": "^1.0.0",
        "d3-timer": "^3.0.1",
        "d3-transition": "^3.0.1",
        "d3-voronoi-treemap": "^1.1.2",
        "d3-weighted-voronoi": "^1.1.3",
        "d3-zoom": "^3.0.0",
        "dayjs": "^1.11.9",
        "hashids": "^2.3.0",
        "htm": "^3.1.1",
        "html2canvas": "^1.4.1",
        "image-promise": "^7.0.1",
        "js-cookie": "^3.0.5",
        "preact": "^10.16.0",
        "preact-render-to-string": "^6.2.0",
        "round-slider": "^1.6.1",
        "seedrandom": "^3.0.5",
        "textures": "^1.2.3",
        "timelines-chart": "^2.12.1",
        "yarn-run-all": "^3.1.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.9",
        "@babel/core": "^7.22.9",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-private-methods": "^7.18.6",
        "@babel/preset-env": "^7.22.9",
        "@babel/preset-typescript": "^7.22.5",
        "@commitlint/cli": "^17.6.7",
        "@commitlint/config-conventional": "^17.6.7",
        "@fullhuman/postcss-purgecss": "^5.0.0",
        "@types/bootstrap": "^5.2.6",
        "@types/bootstrap-select": "^1.13.4",
        "@types/css": "^0.0.33",
        "@types/d3-array": "^3.0.5",
        "@types/d3-axis": "^3.0.2",
        "@types/d3-color": "^3.1.0",
        "@types/d3-delaunay": "^6.0.1",
        "@types/d3-drag": "^3.0.2",
        "@types/d3-format": "^3.0.1",
        "@types/d3-hierarchy": "^3.1.2",
        "@types/d3-interpolate": "^3.0.1",
        "@types/d3-scale": "^4.0.3",
        "@types/d3-selection": "^3.0.5",
        "@types/d3-shape": "^3.1.1",
        "@types/d3-timer": "^3.0.0",
        "@types/d3-transition": "^3.0.3",
        "@types/d3-zoom": "^3.0.3",
        "@types/eslint": "^8.44.0",
        "@types/google.analytics": "^0.0.42",
        "@types/got": "^9.6.12",
        "@types/jquery": "^3.5.16",
        "@types/js-cookie": "^3.0.3",
        "@types/lzma-native": "^4.0.1",
        "@types/node": "^20.4.4",
        "@types/pngjs": "^6.0.1",
        "@types/polylabel": "^1.1.0",
        "@types/recursive-readdir": "^2.2.1",
        "@types/resize-observer-browser": "^0.1.7",
        "@types/seedrandom": "^3.0.5",
        "@types/webpack-env": "^1.18.1",
        "@types/yargs": "^17.0.24",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "autoprefixer": "^10.4.14",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "core-js": "^3.31.1",
        "css": "^3.0.0",
        "css-loader": "^6.8.1",
        "css-minimizer-webpack-plugin": "^5.0.1",
        "csso": "^5.0.5",
        "diff2html-cli": "^5.2.9",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "favicons": "^7.1.3",
        "favicons-webpack-plugin": "^6.0.0",
        "html-webpack-plugin": "^5.5.3",
        "husky": "^8.0.3",
        "mini-css-extract-plugin": "^2.7.6",
        "mini-svg-data-uri": "^1.4.4",
        "netlify-cli": "^15.9.1",
        "postcss": "^8.4.27",
        "postcss-cli": "^10.1.0",
        "postcss-loader": "^7.3.3",
        "prettier": "^3.0.0",
        "prettier-package-json": "^2.8.0",
        "sass": "^1.64.1",
        "sass-loader": "^13.3.2",
        "shellcheck": "^2.2.0",
        "simple-statistics": "^7.8.3",
        "sitemap-webpack-plugin": "^1.1.1",
        "standard-version": "^9.5.0",
        "stylelint": "^15.10.2",
        "stylelint-config-recommended-vue": "^1.5.0",
        "stylelint-config-standard-scss": "^10.0.0",
        "stylelint-order": "^6.0.3",
        "stylelint-prettier": "^4.0.0",
        "svgo": "^3.0.2",
        "terser-webpack-plugin": "^5.3.9",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "utility-types": "^3.10.0",
        "webpack": "^5.88.2",
        "webpack-bundle-analyzer": "^4.9.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "webpack-subresource-integrity": "^5.2.0-rc.1",
        "yargs": "^17.7.2"
    },
    "engines": {
        "node": ">= 18"
    },
    "browserslist": [
        "defaults and supports es6-module and last 3 versions"
    ],
    "prettier": {
        "printWidth": 120,
        "semi": false,
        "tabWidth": 4
    },
    "prettier-package-json": {
        "tabWidth": 4
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ],
        "rules": {
            "subject-case": [
                2,
                "never",
                [
                    "start-case",
                    "pascal-case",
                    "upper-case"
                ]
            ],
            "type-enum": [
                2,
                "always",
                [
                    "build",
                    "chore",
                    "ci",
                    "docs",
                    "feat",
                    "fix",
                    "perf",
                    "refactor",
                    "revert",
                    "style",
                    "test"
                ]
            ]
        }
    }
}
