{
    "name": "na-map-one",
    "packageManager": "yarn@4.0.0",
    "version": "15.1.7",
    "description": "Naval Action map",
    "scripts": {
        "build:push": "git push",
        "clean:src:js": "find ./src/js -name \"*.ts\" -type f -regextype posix-egrep -regex \".*/(browser|common|node)/.+\" -exec bash -c 'for file in \"${1%.ts}\".js \"${1%.ts}\".js.map; do [ -e \"$file\" ] && rm -f \"$file\"; done' - '{}' ';'",
        "deploy:regular": "src/helper/deploy.sh --site regular",
        "deploy:test": "src/helper/deploy.sh --site test1",
        "docs": "run-p 'docs:*'",
        "docs:js": "documentation build src/js/**.js build/**.mjs -f html -o ./doc-js",
        "docs:sass": "sassdoc src/scss -d ./doc-sass",
        "helper:loc": "npx cloc --by-file --not-match-f='.d.ts' --quiet ./src/js",
        "helper:size:css": "find public/ -iname '*.css' -exec du -bc {} + | sort -k 2 | sed 's|public/||'",
        "helper:size:js": "find public/ -iname '*.js' -not -iname 'data-*.js' -exec du -bc {} + | sort -k 2 | sed 's|public/||'",
        "helper:size:largest": "du -a ~/ -BM | sort -n -r | head -n 15",
        "lint": "run-s 'lint:*'",
        "lint:css": "stylelint --config .stylelintrc \"src/scss/*.scss\"",
        "lint:ejs": "ejslint src/ejs",
        "lint:js": "eslint -c .eslintrc.cjs --ext .ts,.tsx src/js",
        "main:develop-test": "TARGET=na-map-test    run-s         prod deploy:test    build:push",
        "main:release": "     TARGET=na-map         run-s release prod deploy:regular build:push",
        "prod": "TS_NODE_PROJECT='webpack/tsconfig-for-webpack-config.json' webpack --config webpack/webpack.config.ts --mode=production --color",
        "release": "release-it --ci",
        "start": "TS_NODE_PROJECT='webpack/tsconfig-for-webpack-config.json' webpack serve --config webpack/webpack.config.ts --mode=development --color --hot --open",
        "typecheck": "tsc --noEmit",
        "update": "yarn set version stable && yarn upgrade-interactive"
    },
    "sass": "src/scss/main.scss",
    "dependencies": {
        "@d3fc/d3fc-label-layout": "^7.0.3",
        "@popperjs/core": "^2.11.8",
        "bootstrap": "5.2.3",
        "bootstrap-select": "https://github.com/felixvictor/bootstrap-select#dev",
        "d3-area-label": "^1.5.0",
        "d3-array": "^3.2.4",
        "d3-axis": "^3.0.0",
        "d3-color": "^3.1.0",
        "d3-delaunay": "^6.0.4",
        "d3-drag": "^3.0.0",
        "d3-format": "^3.1.0",
        "d3-hierarchy": "^3.1.2",
        "d3-interpolate": "^3.0.1",
        "d3-scale": "^4.0.2",
        "d3-selection": "^3.0.0",
        "d3-shape": "^3.2.0",
        "d3-tile": "^1.0.0",
        "d3-timer": "^3.0.1",
        "d3-transition": "^3.0.1",
        "d3-voronoi-treemap": "^1.1.2",
        "d3-weighted-voronoi": "^1.1.3",
        "d3-zoom": "^3.0.0",
        "dayjs": "^1.11.10",
        "hashids": "^2.3.0",
        "htm": "^3.1.1",
        "html2canvas": "^1.4.1",
        "image-promise": "^7.0.1",
        "js-cookie": "^3.0.5",
        "preact": "^10.18.1",
        "preact-render-to-string": "^6.2.2",
        "round-slider": "^1.6.1",
        "seedrandom": "^3.0.5",
        "textures": "^1.2.3",
        "timelines-chart": "^2.12.1",
        "yarn-run-all": "^3.1.1"
    },
    "devDependencies": {
        "@adobe/css-tools": "^4.3.1",
        "@babel/cli": "^7.23.0",
        "@babel/core": "^7.23.2",
        "@babel/plugin-transform-class-properties": "^7.22.5",
        "@babel/plugin-transform-private-methods": "^7.22.5",
        "@babel/preset-env": "^7.23.2",
        "@babel/preset-typescript": "^7.23.2",
        "@commitlint/cli": "^18.2.0",
        "@commitlint/config-conventional": "^18.1.0",
        "@fullhuman/postcss-purgecss": "^5.0.0",
        "@release-it/conventional-changelog": "^7.0.2",
        "@types/bootstrap": "^5.2.8",
        "@types/bootstrap-select": "^1.13.5",
        "@types/d3-array": "^3.0.9",
        "@types/d3-axis": "^3.0.5",
        "@types/d3-color": "^3.1.2",
        "@types/d3-delaunay": "^6.0.3",
        "@types/d3-drag": "^3.0.5",
        "@types/d3-format": "^3.0.3",
        "@types/d3-hierarchy": "^3.1.5",
        "@types/d3-interpolate": "^3.0.3",
        "@types/d3-scale": "^4.0.6",
        "@types/d3-selection": "^3.0.8",
        "@types/d3-shape": "^3.1.4",
        "@types/d3-timer": "^3.0.1",
        "@types/d3-transition": "^3.0.6",
        "@types/d3-zoom": "^3.0.6",
        "@types/eslint": "^8.44.6",
        "@types/google.analytics": "^0.0.44",
        "@types/got": "^9.6.12",
        "@types/jquery": "^3.5.25",
        "@types/js-cookie": "^3.0.5",
        "@types/lzma-native": "^4.0.3",
        "@types/node": "^20.8.9",
        "@types/pngjs": "^6.0.3",
        "@types/polylabel": "^1.1.2",
        "@types/recursive-readdir": "^2.2.3",
        "@types/resize-observer-browser": "^0.1.9",
        "@types/seedrandom": "^3.0.7",
        "@types/webpack-env": "^1.18.3",
        "@types/yargs": "^17.0.29",
        "@typescript-eslint/eslint-plugin": "^6.9.0",
        "@typescript-eslint/parser": "^6.9.0",
        "autoprefixer": "^10.4.16",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "core-js": "^3.33.1",
        "css-loader": "^6.8.1",
        "css-minimizer-webpack-plugin": "^5.0.1",
        "csso": "^5.0.5",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "favicons": "^7.1.4",
        "favicons-webpack-plugin": "^6.0.1",
        "html-webpack-plugin": "^5.5.3",
        "husky": "^8.0.3",
        "mini-css-extract-plugin": "^2.7.6",
        "mini-svg-data-uri": "^1.4.4",
        "netlify-cli": "^16.9.3",
        "postcss": "^8.4.31",
        "postcss-cli": "^10.1.0",
        "postcss-loader": "^7.3.3",
        "prettier": "^3.0.3",
        "prettier-package-json": "^2.8.0",
        "release-it": "^16.2.1",
        "sass": "^1.69.5",
        "sass-loader": "^13.3.2",
        "shellcheck": "^2.2.0",
        "sitemap-webpack-plugin": "^1.1.1",
        "stylelint": "^15.11.0",
        "stylelint-order": "^6.0.3",
        "stylelint-prettier": "^4.0.2",
        "svgo": "^3.0.2",
        "terser-webpack-plugin": "^5.3.9",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2",
        "utility-types": "^3.10.0",
        "webpack": "^5.89.0",
        "webpack-bundle-analyzer": "^4.9.1",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "webpack-subresource-integrity": "^5.2.0-rc.1",
        "yargs": "^17.7.2"
    },
    "resolutions": {
        "readdirp": "^3.6.0"
    },
    "engines": {
        "node": ">= 20"
    },
    "browserslist": [
        "defaults and supports es6-module and last 3 versions"
    ],
    "prettier": {
        "printWidth": 120,
        "semi": false,
        "tabWidth": 4
    },
    "prettier-package-json": {
        "tabWidth": 4
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ],
        "rules": {
            "subject-case": [
                2,
                "never",
                [
                    "start-case",
                    "pascal-case",
                    "upper-case"
                ]
            ],
            "type-enum": [
                2,
                "always",
                [
                    "build",
                    "chore",
                    "ci",
                    "docs",
                    "feat",
                    "fix",
                    "perf",
                    "refactor",
                    "revert",
                    "style",
                    "test"
                ]
            ]
        }
    },
    "release-it": {
        "git": {
            "commitMessage": "chore: release v${version}",
            "push": false
        },
        "github": {
            "release": false
        },
        "gitlab": {
            "release": false
        },
        "npm": {
            "publish": false
        },
        "plugins": {
            "@release-it/conventional-changelog": {
                "infile": "CHANGELOG.md",
                "preset": "conventionalcommits"
            }
        }
    }
}
