{
    "name": "na-map-one",
    "version": "15.2.5",
    "description": "Naval Action map",
    "scripts": {
        "build:push": "git push",
        "clean:cache": "yarn cache clean --all && rm -rf node_modules yarn.lock .pnp/ .pnp.cjs .pnp.loader.mjs .yarn/{install-state.gz,unplugged,cache} && yarn",
        "clean:src:js": "find ./src/js -name \"*.ts\" -type f -regextype posix-egrep -regex \".*/(browser|common|node)/.+\" -exec bash -c 'for file in \"${1%.ts}\".js \"${1%.ts}\".js.map; do [ -e \"$file\" ] && rm -f \"$file\"; done' - '{}' ';'",
        "deploy:regular": "src/helper/deploy.sh --site regular",
        "deploy:test": "src/helper/deploy.sh --site test1",
        "docs": "run-p 'docs:*'",
        "docs:js": "documentation build src/js/**.js build/**.mjs -f html -o ./doc-js",
        "docs:sass": "sassdoc src/scss -d ./doc-sass",
        "git:chore": "git add package.json yarn.lock .yarn .yarnrc.yml && git commit -m 'chore(deps): bump'",
        "helper:build:tiles": "vips dzsave /mnt/na-map.tif src/images/map --depth onetile --layout google --tile-size 256 --overlap 0 --suffix .webp[Q=100,reduction-effort=6]",
        "helper:loc": "npx cloc --by-file --not-match-f='.d.ts' --quiet ./src/js",
        "helper:size:css": "find public/ -iname '*.css' -exec du -bc {} + | sort -k 2 | sed 's|public/||'",
        "helper:size:js": "find public/ -iname '*.js' -not -iname 'data-*.js' -exec du -bc {} + | sort -k 2 | sed 's|public/||'",
        "helper:size:largest": "du -a ~/ -BM | sort -n -r | head -n 15",
        "lint": "run-s 'lint:*' pretty-print pretty-print-package",
        "lint:css": "stylelint --fix --config .stylelintrc \"src/scss/*.scss\"",
        "lint:ejs": "ejslint src/ejs",
        "lint:js": "eslint . --ext .js,.cjs,.mjs,.ts,.cts,.mts,.jsx,.tsx,.vue --fix",
        "main:develop-test": "TARGET=na-map-test    run-s         prod deploy:test    build:push",
        "main:release": "     TARGET=na-map         run-s release prod deploy:regular build:push",
        "pretty-print": "yarn prettier --config prettier.config.cjs --log-level error --write .",
        "pretty-print-package": "yarn prettier-package-json --write ./package.json",
        "prod": "TS_NODE_PROJECT='webpack/tsconfig-for-webpack-config.json' webpack --config webpack/webpack.config.ts --mode=production --color",
        "release": "release-it --ci",
        "start": "TS_NODE_PROJECT='webpack/tsconfig-for-webpack-config.json' webpack serve --config webpack/webpack.config.ts --mode=development --color --hot --open",
        "typecheck": "tsc --noEmit",
        "update": "yarn set version stable && yarn upgrade-interactive"
    },
    "resolutions": {
        "readdirp": "^3.6.0"
    },
    "dependencies": {
        "@d3fc/d3fc-label-layout": "^7.0.4",
        "@popperjs/core": "^2.11.8",
        "bootstrap": "5.2.3",
        "bootstrap-select": "https://github.com/felixvictor/bootstrap-select#dev",
        "d3-area-label": "^1.6.0",
        "d3-array": "^3.2.4",
        "d3-axis": "^3.0.0",
        "d3-color": "^3.1.0",
        "d3-delaunay": "^6.0.4",
        "d3-drag": "^3.0.0",
        "d3-format": "^3.1.0",
        "d3-hierarchy": "^3.1.2",
        "d3-interpolate": "^3.0.1",
        "d3-scale": "^4.0.2",
        "d3-selection": "^3.0.0",
        "d3-shape": "^3.2.0",
        "d3-tile": "^1.0.0",
        "d3-timer": "^3.0.1",
        "d3-transition": "^3.0.1",
        "d3-voronoi-treemap": "^1.1.2",
        "d3-weighted-voronoi": "^1.1.3",
        "d3-zoom": "^3.0.0",
        "dayjs": "^1.11.12",
        "hashids": "^2.3.0",
        "htm": "^3.1.1",
        "html2canvas": "^1.4.1",
        "image-promise": "^8.0.0",
        "js-cookie": "^3.0.5",
        "preact": "^10.23.1",
        "preact-render-to-string": "^6.5.7",
        "round-slider": "^1.6.1",
        "seedrandom": "^3.0.5",
        "textures": "^1.2.3",
        "timelines-chart": "^2.12.1",
        "yarn-run-all": "^3.1.1"
    },
    "devDependencies": {
        "@adobe/css-tools": "^4.4.0",
        "@babel/cli": "^7.24.8",
        "@babel/core": "^7.24.9",
        "@babel/plugin-transform-class-properties": "^7.24.7",
        "@babel/plugin-transform-private-methods": "^7.24.7",
        "@babel/preset-env": "^7.25.0",
        "@babel/preset-typescript": "^7.24.7",
        "@commitlint/cli": "^19.3.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@fullhuman/postcss-purgecss": "^6.0.0",
        "@release-it/conventional-changelog": "^8.0.1",
        "@types/bootstrap": "^5.2.10",
        "@types/bootstrap-select": "^1.13.7",
        "@types/d3-array": "^3.2.1",
        "@types/d3-axis": "^3.0.6",
        "@types/d3-color": "^3.1.3",
        "@types/d3-delaunay": "^6.0.4",
        "@types/d3-drag": "^3.0.7",
        "@types/d3-format": "^3.0.4",
        "@types/d3-hierarchy": "^3.1.7",
        "@types/d3-interpolate": "^3.0.4",
        "@types/d3-scale": "^4.0.8",
        "@types/d3-selection": "^3.0.10",
        "@types/d3-shape": "^3.1.6",
        "@types/d3-timer": "^3.0.2",
        "@types/d3-transition": "^3.0.8",
        "@types/d3-zoom": "^3.0.8",
        "@types/eslint": "^8.56.10",
        "@types/google.analytics": "^0.0.46",
        "@types/got": "^9.6.12",
        "@types/jquery": "^3.5.30",
        "@types/js-cookie": "^3.0.6",
        "@types/lzma-native": "^4.0.4",
        "@types/node": "^20.14.12",
        "@types/pngjs": "^6.0.5",
        "@types/polylabel": "^1.1.3",
        "@types/recursive-readdir": "^2.2.4",
        "@types/resize-observer-browser": "^0.1.11",
        "@types/seedrandom": "^3.0.8",
        "@types/webpack-env": "^1.18.5",
        "@types/yargs": "^17.0.32",
        "@typescript-eslint/eslint-plugin": "^7.17.0",
        "@typescript-eslint/parser": "^7.17.0",
        "autoprefixer": "^10.4.19",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^12.0.2",
        "core-js": "^3.37.1",
        "css-loader": "^7.1.2",
        "css-minimizer-webpack-plugin": "^7.0.0",
        "csso": "^5.0.5",
        "ejs-lint": "^2.0.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "favicons": "^7.2.0",
        "favicons-webpack-plugin": "^6.0.1",
        "html-webpack-plugin": "^5.6.0",
        "husky": "^9.1.3",
        "mini-css-extract-plugin": "^2.9.0",
        "mini-svg-data-uri": "^1.4.4",
        "netlify-cli": "^17.33.4",
        "postcss": "^8.4.40",
        "postcss-cli": "^11.0.0",
        "postcss-loader": "^8.1.1",
        "prettier": "^3.3.3",
        "prettier-package-json": "^2.8.0",
        "prettier-plugin-organize-imports": "^3.2.4",
        "release-it": "^17.6.0",
        "sass": "^1.77.8",
        "sass-loader": "^16.0.0",
        "shellcheck": "^3.0.0",
        "sitemap-webpack-plugin": "^1.1.1",
        "stylelint": "^16.7.0",
        "stylelint-config-standard-scss": "^13.1.0",
        "stylelint-order": "^6.0.4",
        "stylelint-prettier": "^5.0.2",
        "svgo": "^3.3.2",
        "terser-webpack-plugin": "^5.3.10",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4",
        "utility-types": "^3.11.0",
        "webpack": "^5.93.0",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4",
        "webpack-subresource-integrity": "^5.2.0-rc.1",
        "yargs": "^17.7.2"
    },
    "engines": {
        "node": ">= 20"
    },
    "browserslist": [
        "defaults and supports es6-module and last 3 versions"
    ],
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ],
        "rules": {
            "subject-case": [
                2,
                "never",
                [
                    "start-case",
                    "pascal-case",
                    "upper-case"
                ]
            ],
            "type-enum": [
                2,
                "always",
                [
                    "build",
                    "chore",
                    "ci",
                    "docs",
                    "feat",
                    "fix",
                    "perf",
                    "refactor",
                    "revert",
                    "style",
                    "test"
                ]
            ]
        }
    },
    "packageManager": "yarn@4.3.1",
    "prettier-package-json": {
        "tabWidth": 4
    },
    "release-it": {
        "git": {
            "commitMessage": "chore: release v${version}",
            "push": false
        },
        "github": {
            "release": false
        },
        "gitlab": {
            "release": false
        },
        "npm": {
            "publish": false
        },
        "plugins": {
            "@release-it/conventional-changelog": {
                "infile": "CHANGELOG.md",
                "preset": "conventionalcommits"
            }
        }
    },
    "sass": "src/scss/main.scss"
}
